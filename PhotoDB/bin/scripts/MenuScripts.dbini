  <Скрипты>[DB_IC_SHELL]
  {
    <Список функций>[DB_IC_OPTIONS]
    {
     ShowFunctionList;
    }
    <Список переменных>[DB_IC_BOX]
    {
     ShowMemoryStatus;
    }
    <Открыть папку>[DB_IC_DIRECTORY]
    {
     $script_path=GetProgramPath;
     $script_path=ExtractFileDirectory($script_path);
     $script_path=$script_path+"\scripts";
     NewExplorerByPath($script_path);
    }
  }